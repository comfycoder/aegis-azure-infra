@model MyWebApp.Models.HomeViewModel

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">My Web App</h1>
    <ul style="list-style-type: none;">
        <li><b>Host Name:</b> @Model.HostName</li>
        <li><b>Environment:</b> @Model.EnvironmentName</li>
        <li><b>My Setting:</b> @Model.MySetting</li>
        <li><b>Key Vault Name:</b> @Model.KeyVaultName</li>
        <li><b>Secret 1:</b> @Model.Secret1</li>
        <li><b>Secret 2:</b> @Model.Secret2</li>
    </ul>
</div>
